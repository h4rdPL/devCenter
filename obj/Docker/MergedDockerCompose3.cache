name: dockercompose9077296373072857997
services:
  devcenter.api:
    build:
      context: D:\_programowanie\devCenter
      dockerfile: DevCenter.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: devcenterapi
    networks:
      default: null
